@using TimelessTreasuresWeb1.ServiceReference1


@{

    Service1Client client = new Service1Client();

    var myChart = new Chart(width: 800, height: 800)
        .AddTitle("Items on Hand Per Product")
        .AddSeries(
            name: "Product",
            chartType: "Bar",
            xValue: client.getItemNames(),
            yValues: client.getItemOnHand(),
            markerStep: 1)
        .SetYAxis(
            title: "Products")
        .SetXAxis(
            title: "Number of on hand")
        .Write();
}

